{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "react/jsx-one-expression-per-line": [
      0,
      {
        "allow": "none"
      }
    ],
    "linebreak-style": [
      0,
      {
        "allow": "none"
      }
    ],
    "import/named": 0,
    "func-names": 0,
    "import/no-named-as-default": 0
  }
}